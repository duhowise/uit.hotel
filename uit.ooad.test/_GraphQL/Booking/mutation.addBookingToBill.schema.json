{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "required": [
      "data"
    ],
    "properties": {
      "data": {
        "type": "object",
        "required": [
          "addBookingToBill"
        ],
        "properties": {
          "addBookingToBill": {
            "type": "object",
            "required": [
              "id",
              "bookCheckInTime",
              "bookCheckOutTime",
              "realCheckInTime",
              "realCheckOutTime",
              "createTime",
              "status",
              "room",
              "employeeBooking",
              "employeeCheckIn",
              "employeeCheckOut",
              "bill",
              "patrons"
            ],
            "properties": {
              "id": {
                "type": "integer"
              },
              "bookCheckInTime": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "bookCheckOutTime": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "realCheckInTime": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "realCheckOutTime": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "createTime": {
                "type": "string",
                "pattern": "^(.*)$"
              },
              "status": {
                "type": "integer"
              },
              "room": {
                "type": "object",
                "required": [
                  "id"
                ],
                "properties": {
                  "id": {
                    "type": "integer"
                  }
                }
              },
              "employeeBooking": {
                "type": "object",
                "required": [
                  "id"
                ],
                "properties": {
                  "id": {
                    "type": "string",
                    "pattern": "^(.*)$"
                  }
                }
              },
              "employeeCheckIn": {
                "type": "null"
              },
              "employeeCheckOut": {
                "type": "null"
              },
              "bill": {
                "type": "object",
                "required": [
                  "id"
                ],
                "properties": {
                  "id": {
                    "type": "integer"
                  }
                }
              },
              "patrons": {
                "type": "array",
                "items": {
                  "type": "object",
                  "required": [
                    "id"
                  ],
                  "properties": {
                    "id": {
                      "type": "integer"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }